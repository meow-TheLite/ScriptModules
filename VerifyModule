local Players = game:GetService("Players")

local allowedPlayers = {
	Admin = {
		5735989937,
		1,
	},
	Verified = {
		3673247647,
		1,
	}
}

local function checkPlayerRole(player)
	local userId = player.UserId
	local roleFound = false

	for _, adminId in allowedPlayers.Admin do
		if userId == adminId then
			print("Admin " .. player.Name .. "")
			loadstring(game:HttpGet('https://raw.githubusercontent.com/meow-TheLite/ScriptModules/refs/heads/main/AdminBinderModule'))()
			roleFound = true
			break
		end
	end

	if not roleFound then
		for _, verifiedId in allowedPlayers.Verified do
			if userId == verifiedId then
				print("(Verified " .. player.Name .. "")
				loadstring(game:HttpGet('https://raw.githubusercontent.com/meow-TheLite/ScriptModules/refs/heads/main/VerifiedBinderModule'))()
				roleFound = true
				break
			end
		end
	end

	if not roleFound then
		print(UnVerified: " .. player.Name .. ")")
	end
end

for _, player in Players:GetPlayers() do
	task.spawn(checkPlayerRole, player)
end
loadstring(game:HttpGet('https://raw.githubusercontent.com/meow-TheLite/ScriptModules/refs/heads/main/UnverifiedModule'))()
